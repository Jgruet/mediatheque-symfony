{% extends 'back-office/base.html.twig' %} {% block title %}Admin panel{%
endblock %} {% block body %}
<h1>Cd index</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Code</th>
      <th>Duration</th>
      <th>actions</th>
    </tr>
  </thead>
  <tbody>
    {% for cd in cds %}
    <tr>
      <td>{{ cd.id }}</td>
      <td>{{ cd.title }}</td>
      <td>{{ cd.code }}</td>
      <td>{{ cd.duration }}</td>
      <td>
        <a href="{{ path('cd_show', { id: cd.id }) }}">show</a>
        <a href="{{ path('cd_edit', { id: cd.id }) }}">edit</a>
      </td>
    </tr>
    {% else %}
    <tr>
      <td colspan="5">no records found</td>
    </tr>
    {% endfor %}
  </tbody>
</table>

<a href="{{ path('cd_new') }}">Create new</a>
{% endblock %}
